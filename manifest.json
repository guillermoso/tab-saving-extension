{
    "name": "Tab saver",
    "short_name": "Tab saver",
    "version": "1.0.0",
    "description": "Save your tabs for later!",
    "manifest_version": 2,
    "icons": {
        "16": "./images/16x16.png",
        "48": "./images/48x48.png",
        "128": "./images/128x128.png"
    },
    "browser_action": {
        "default_icon": "./images/16x16.png",
        "default_popup": "popup.html"
    },
    "permissions": ["tabs", "storage"],
    "author": "Guillermo Alvarez"
}