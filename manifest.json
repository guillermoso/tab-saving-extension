{
    "name": "Tab saver",
    "version": "1.0.0",
    "description": "Save your tabs for later!",
    "manifest_version": 2,
    "icons": { "128": "./images/150_x_150_Stars.png"},
    "browser_action": {
        "default_icon": "./images/19_x_19.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "identity", "identity.email"]
}